{
    "name": "refinery29/php-cs-fixer-config",
    "description": "Provides a configuration for friendsofphp/php-cs-fixer, used within Refinery29.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas MÃ¶ller",
            "email": "andreas.moller@refinery29.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^5.5 || ^7.0",
        "friendsofphp/php-cs-fixer": "^2.0.0"
    },
    "require-dev": {
        "codeclimate/php-test-reporter": "0.3.2",
        "phpunit/phpunit": "^4.8.28"
    },
    "autoload": {
        "psr-4": {
            "Refinery29\\CS\\Config\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Refinery29\\CS\\Config\\Test\\": "test"
        }
    }
}
